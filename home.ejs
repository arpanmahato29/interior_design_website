      <!-- For scrolling page left and right -->
      <script>
        $(function(){

          $("#scrl_left").click(function(){
            $("#furniture_layout").animate({scrollLeft:'-=280px'},600);
          });

          $("#scrl_right").click(function(){
            $("#furniture_layout").animate({scrollLeft:'+=280px'},600);
          });

          $("#scrl_left").hover(function(){
            $(this).find("i").css({'transition':'0.4s ease-in-out','position':'relative','right':'7px','color':'white'});
            $(this).css({'background':'#004c56','transition':'0.4s ease-in-out','border':'1px solid transparent'});
          },function(){
             $(this).find("i").css({'position':'relative','right':'0px','color':'black'});
             $(this).css({'background':'transparent','transition':'0.4s ease-in-out','border':'1px solid #222830'});
          });

          $("#scrl_right").hover(function(){
            $(this).find("i").css({'color':'white','position':'relative','left':'10px','transition':'0.4s'},'slow');
            $(this).css({'background':'#004c56','transition':'0.4s ease-in-out','border':'1px solid transparent'});
          },function(){
            $(this).find("i").css({'color':'black','position':'relative','left':'0px'},'slow');
            $(this).css({'background':'transparent','transition':'0.4s ease-in-out','border':'1px solid #222830'});
          });
          
        });
      </script>


